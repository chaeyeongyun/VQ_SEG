{   
    "project_name":"OBIA_CWFID",
    "wandb_logging":true,
    "num_classes":3,
    "pixel_to_label":{
        "0":0,
        "128":1,
        "255":2
    },
    "resize":512,
    "model":{
        "name":"fcn32s",
        "params":{
            "encoder_name":"vgg19",
            "num_classes":3,
            "encoder_weights":"imagenet"
            }
    },
    "train":{
        "data_dir":"../data/semi_sup_data/CWFID/num30",
        "save_dir":"../drive/MyDrive/related_work/CWFID",
        "device":0,
        "num_epochs":400,
        "batch_size":8,
        "learning_rate":5e-5,
        "lr_scheduler":{
            "name":"cosineannealing",
            "min_lr":1e-8,
            "warmup_steps":0
        },
        "half":true,
        "init_weights":true,
        "bn_eps":1e-5,
        "bn_momentum":0.1,
        "criterion":{
            "name":"cross_entropy"
        },
        "save_img":true,
        "save_txt":true,
        "save_as_tar":false,
        "wandb_config":["batch_size", "num_epochs"],
        "wandb_log":["loss", "learning_rate", "miou", "crop_iou", "weed_iou", "back_iou"],
        "wandb_metrics":["loss", "miou"] 
        },
    "test":{
        "data_dir":"../data/semi_sup_data/CWFID/num30",
        "save_dir":"../drive/MyDrive/related_work/CWFID",
        "device":0,
        "batch_size":1,
        "weights":"../drive/MyDrive/only_sup_train/CWFID/"
    }
}
